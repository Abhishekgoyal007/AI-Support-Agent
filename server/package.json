{
    "name": "server",
    "version": "1.0.0",
    "description": "Backend API for Spur AI Support Agent",
    "main": "dist/index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "npx prisma generate && npx prisma migrate deploy && npm run db:seed",
        "start": "tsx src/index.ts",
        "db:migrate": "prisma migrate dev",
        "db:seed": "tsx prisma/seed.ts",
        "db:reset": "prisma migrate reset --force",
        "db:studio": "prisma studio"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@prisma/client": "^5.22.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "uuid": "^11.0.3",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/node": "^22.10.2",
        "@types/uuid": "^10.0.0",
        "prisma": "^5.22.0",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2"
    }
}